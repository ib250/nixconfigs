set nocompatible
filetype off

set rtp+=~/.vim/bundle/Vundle.vim

call vundle#begin()

Plugin 'VisIncr'
Plugin 'UltiSnips'
Plugin 'vim-snippets'
Plugin 'Solarized'
Plugin 'vim-addon-nix'
Plugin 'vim-addon-completion'
Plugin 'vim-addon-manager'
Plugin 'Syntastic'
Plugin 'ctrlp'
Plugin 'The_NERD_Commenter'
Plugin 'The_NERD_tree'
Plugin 'surround'
Plugin 'rainbow_parentheses'
Plugin 'vim-nix'
Plugin 'vim-indent-guides'
Plugin 'vim-repeat'
Plugin 'https://github.com/lambdatoast/elm.vim.git'

call vundle#end()
filetype plugin indent on    " required
